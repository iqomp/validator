{
    "name": "iqomp/validator",
    "description": "Simple and easy form/object validator",
    "type": "library",
    "license": "MIT",
    "version": "2.3.1",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {},
    "extra": {
        "hyperf": {
            "config": "Iqomp\\Validator\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\Validator\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    },
    "suggest": {
        "hyperf/translation": "Allow enum to be translated to id-ID or en-US"
    }
}
